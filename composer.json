{
  "name": "dmoed/fb-like-button",
  "description": "Bolt Extension to add a Facebook Like Button",
  "type": "bolt-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Don Moedjijo",
      "email": "dmoedjijo@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "twig/twig": "^2.12 | ^3.0"
  },
  "require-dev": {
    "bolt/core": "^4.0 | ^5.0",
    "franzl/studio": "^0.15.0"
  },
  "autoload": {
    "psr-4": {
      "dmoed\\FbLikeButton\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "entrypoint": "dmoed\\FbLikeButton\\Extension"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "drupol/composer-packages": true,
      "franzl/studio": true
    }
  }
}
